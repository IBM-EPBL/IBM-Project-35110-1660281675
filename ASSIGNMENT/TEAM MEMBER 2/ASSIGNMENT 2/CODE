import random
while(True):
    t=random.randint(10,99)
    h=random.randint(10,99)
    if(t>30 and h<40):
        print("\nHigh temperature and values of Temperature & Humidity is:",t,h,"Alarm is on")
    elif(t<30 and h>40):
        print("\nLow  temperature and values of Temperature & Humidity is:",t,h,"Alarm is off")
